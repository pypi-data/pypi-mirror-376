{
    "search.useIgnoreFiles": false,
    "files.exclude": {
        "**/.git": false,
        "**/.svn": false,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "__pycache__/": true,
        ".pytest_cache/": true,
        "**/__pycache__/**": false,
        "requirements*": true,
        "**/*.DS_Store": true,
        "**/*__init__.py": false,
        "**/*py.typed": true,
        ".history": true,
        "**/CVS": false,
        "**/.retool_types/**": true,
        "**/*tsconfig.json": true,
        ".cache": true,
        "retool.config.json": true
    },
    "python.defaultInterpreterPath": "${workspaceFolder}/.venv/bin/python",
    "python.analysis.diagnosticSeverityOverrides": {
        "reportUnusedImport": false
    },
    "python.testing.pytestArgs": [
        "tests",
        "--color=yes"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "markdownlint.config": {
        "default": true,
        "MD013": false,
        "MD033": false
    },
    "C_Cpp.dimInactiveRegions": false,
    "cSpell.words": [
        "adipisicing",
