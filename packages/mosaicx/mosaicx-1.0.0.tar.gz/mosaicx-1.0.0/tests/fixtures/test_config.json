{
  "schema": {
    "patient_name": {
      "type": "string",
      "description": "Patient's full name"
    },
    "mrn": {
      "type": "string", 
      "description": "Medical record number"
    },
    "study_date": {
      "type": "string",
      "description": "Date of the study"
    },
    "study_type": {
      "type": "string",
      "description": "Type of medical study"
    },
    "primary_findings": {
      "type": "array",
      "items": {"type": "string"},
      "description": "Primary findings from the report"
    },
    "impression": {
      "type": "string",
      "description": "Radiologist's impression"
    }
  },
  "required_fields": ["patient_name", "study_date", "impression"],
  "output_format": "json",
  "validation_enabled": true,
  "max_retries": 3
}
