#!/usr/bin/env python3

import subprocess
import sys

cmd = sys.argv[1].split()
if cmd[0] == u'get':
    cmd = ['cp', cmd[3], cmd[1]]
elif cmd[0] == u'put':
    cmd = ['cp', cmd[1], cmd[3]]
elif cmd[0] == u'ls':
    # output some expected header lines
    print('one', file=sys.stderr)
    print('two', file=sys.stderr)

sys.exit(subprocess.call(' '.join(cmd) + ' 1>&2', shell=True))
