{
    "python.defaultInterpreterPath": ".venv/bin/python",
    // "python.defaultInterpreterPath": ".pixi/envs/default/bin/python",
    "[python]": {
        "editor.rulers": [
            88
        ]
    },
    "notebook.output.textLineLimit": 50,
    "files.exclude": {
        "**/*.bbl": true,
        "**/*.blg": true,
        "**/*.dvi": true,
        "**/*.fdb_latexmk": true,
        "**/*.fls": true,
        "**/*.aux": true,
        "**/*.synctex.gz": true,
        "**/*.log": true,
        "**/*.out": true,
        "**/*.toc": true,
        "**/*.lof": true,
        "**/*.lot": true,
        "**/*.glsdefs": true,
        "**/_minted-main": true,
        "**/*synctex(busy)": true,
        "**/*.run.xml": true,
        "**/*.bcf": true,
        "**/*.mypy_cache": true,
        "**/*.pytest_cache": true,
        "**/*.ruff_cache": true,
        "**/__pycache__": true,
        "**/*.egg-info": true,
        // envs
        "**/*.venv": true,
        "**/*.pixi": true,
    },
}
