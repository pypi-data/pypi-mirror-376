{
  "model": {
    "name": "MultiModalUNETR",
    "architecture": "unetr",
    "input_size": [
      64,
      64,
      64
    ],
    "in_channels": 4,
    "out_channels": 4,
    "feature_size": 16,
    "hidden_size": 768,
    "mlp_dim": 3072,
    "num_heads": 12,
    "pos_embed": "perceptron",
    "dropout_rate": 0.0
  },
  "training": {
    "batch_size": 1,
    "learning_rate": 0.0001,
    "weight_decay": 1e-05,
    "max_epochs": 50,
    "validation_interval": 1,
    "sw_batch_size": 2,
    "overlap": 0.25,
    "cache_rate": 0.8,
    "num_workers": 4
  },
  "augmentation": {
    "spatial_size": [
      64,
      64,
      64
    ],
    "rand_flip_prob": 0.5,
    "rand_rotate90_prob": 0.5,
    "rand_gaussian_noise_prob": 0.15,
    "rand_gaussian_noise_std": 0.01,
    "rand_scale_intensity_prob": 0.15
  },
  "optimizer": {
    "name": "AdamW",
    "lr": 0.0001,
    "weight_decay": 1e-05
  },
  "loss": {
    "name": "DiceCELoss",
    "to_onehot_y": true,
    "softmax": true
  }
}