{
    "log": {
        "disabled": false,
        "level": "warn",
        "timestamp": false
    },
    "experimental": {
        "clash_api": {
            "external_controller": "127.0.0.1:9090",
            //            "external_ui": "yacd",
            //"external_ui_download_url": "https:\/\/github.com\/MetaCubeX\/Yacd-meta\/archive\/gh-pages.zip",
            //            "external_ui_download_detour": "direct",
            //            "secret": "",
            //            "default_mode": "rule",
            "store_selected": true,
            "cache_file": "clash.db"
        }
    },
    "dns": {
        "servers": [
            {
                "address": "tcp://1.1.1.1",
                "address_resolver": "dns-local",
                "strategy": "prefer_ipv4",
                "tag": "dns-remote",
                "detour": "ssh-out"
            },
            {
                "address": "local",
                "detour": "direct",
                "tag": "dns-local"
            },
            {
                "address": "rcode:\/\/success",
                "tag": "dns-block"
            }
        ],
        "rules": [
            {
                "domain": [
                    "{{domain}}",
                    "github.com",
                    "githubusercontent.com",
                    "{{ssh_ip}}"
                ],
                "server": "dns-local"
            },
            {
                "domain_suffix": [
                    "ir"
                ],
                "server": "dns-local"
            },
            {
                "outbound": "direct",
                "server": "dns-local"
            }
        ],
        "final": "dns-remote",
        "reverse_mapping": true,
        "strategy": "prefer_ipv4",
        "independent_cache": true
    },
    "inbounds": [
        {
            "listen": "127.0.0.1",
            "listen_port": 6450,
            "override_address": "8.8.8.8",
            "override_port": 53,
            "tag": "dns-in",
            "type": "direct"
        },
        {
            "type": "tun",
            "tag": "tun-in",
            "domain_strategy": "prefer_ipv4",
            "interface_name": "tun0",
            "inet4_address": "172.19.0.1\/30",
            "mtu": 9000,
            "auto_route": true,
            "strict_route": true,
            "stack": "system",
            "endpoint_independent_nat": true,
            "sniff": true,
            "sniff_override_destination": false
        },
        {
            "domain_strategy": "prefer_ipv4",
            "listen": "127.0.0.1",
            "listen_port": 2080,
            "sniff": true,
            "sniff_override_destination": false,
            "tag": "mixed-in",
            "type": "mixed"
        }
    ],
    "outbounds": [
        {
            "type": "socks",
            "tag": "SSH+UDP",
            "server": "127.0.0.1",
            "server_port": 2000,
            "version": "5",
            "udp_over_tcp": true,
            "network": "tcp",
            "detour": "ssh-out"
        },
        {
            "type": "ssh",
            "tag": "ssh-out",
            //"client_version": "{{ssh_client_version}}",
            "server": "{{ssh_ip}}",
            "server_port": {{hconfigs[ConfigEnum.ssh_server_port]}},
            "user": "{{user.uuid}}",
            "private_key": "{{user.ed25519_private_key|replace('\n', '\\n')}}", //{{user.ed25519_public_key|replace('\n', '\\n')}}
            "host_key": {{host_keys}}
        },
        {
            "tag": "direct",
            "type": "direct"
        },
        {
            "tag": "bypass",
            "type": "direct"
        },
        {
            "tag": "block",
            "type": "block"
        },
        {
            "tag": "dns-out",
            "type": "dns"
        }
    ],
    "route": {
        "auto_detect_interface": true,
        "override_android_vpn": true,
        "final": "SSH+UDP",
        // "geoip": {
        //             "download_url": "https://{{domain}}/{{hconfigs[ConfigEnum.proxy_path]}}/gh/malikshi/sing-box-geo/releases/latest/download/geoip.db",
        // //    "download_url": "https:\/\/github.com\/malikshi\/sing-box-geo\/releases\/latest\/download\/geoip.db",
        //     "download_detour": "direct"
        // },
        // "geoip": {
        //     "download_url": "https:\/\/github.com\/malikshi\/sing-box-geo\/releases\/latest\/download\/geoip.db",
        //     "download_detour": "direct"
        // },
        // "geosite": {
        //     "download_url": "https:\/\/github.com\/malikshi\/v2ray-rules-dat\/releases\/latest\/download\/geosite.db",
        //     "download_detour": "direct"
        // },
        "rules": [
            {
                "outbound": "dns-out",
                "port": [
                    53
                ]
            },
            {
                "inbound": [
                    "dns-in"
                ],
                "outbound": "dns-out"
            },
            {
                "domain_suffix": [
                    "ir"
                ],
                "outbound": "bypass"
            },
            {
                "geoip": [
                    "ir"
                ],
                "outbound": "bypass"
            },
            {
                "protocol": "quic",
                "port": [443],
                "outbound": "block"
            },
            {
                "ip_cidr": [
                    "224.0.0.0\/3",
                    "ff00::\/8"
                ],
                "outbound": "block",
                "source_ip_cidr": [
                    "224.0.0.0\/3",
                    "ff00::\/8"
                ]
            }
        ]
    }
}
