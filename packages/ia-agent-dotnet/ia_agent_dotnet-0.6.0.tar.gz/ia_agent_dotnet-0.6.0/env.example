# Configuración de IA
# Configura tu API key usando: ia-agent config
OPENAI_API_KEY=your_openai_api_key_here
DEEPSEEK_API_KEY=your_deepseek_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
AI_PROVIDER=deepseek
AI_MODEL=deepseek-coder
AI_TEMPERATURE=0.1
AI_MAX_TOKENS=4000

# Configuración de logging
LOG_LEVEL=INFO
LOG_FILE=./logs/ia_agent.log
LOG_FORMAT=%(asctime)s | %(levelname)-8s | %(name)s:%(funcName)s:%(lineno)d | %(message)s

# Configuración de ChromaDB
CHROMADB_PERSIST_DIRECTORY=./memory/vector
CHROMADB_COLLECTION_NAME=ia_agent_memory
CHROMADB_ANONYMIZED_TELEMETRY=false

# Configuración de .NET
DOTNET_PATH=
DOTNET_TIMEOUT=30

# Configuración de archivos
TEMP_DIRECTORY=./temp
OUTPUT_DIRECTORY=./output
MAX_FILE_SIZE=10485760

# Configuración de rendimiento
MAX_CONCURRENT_AGENTS=3
AGENT_TIMEOUT=60
MEMORY_CACHE_SIZE=1000

# Configuración de seguridad
ENABLE_FILE_VALIDATION=true
ALLOWED_FILE_EXTENSIONS=.cs,.csproj,.sln

# Configuración de desarrollo
DEBUG_MODE=false
ENABLE_TELEMETRY=false