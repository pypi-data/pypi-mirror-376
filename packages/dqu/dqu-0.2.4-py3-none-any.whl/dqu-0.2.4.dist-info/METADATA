dqu_main = DquDataFrame(main_ds, columns=main_columns)

# 3. Reference dataset (e.g., users)
ref_data = [
    (101,),
    (102,),
    (103,),
    (104,),
]
ref_columns = ["id"]
ref_type_info = Types.ROW([Types.INT()])
