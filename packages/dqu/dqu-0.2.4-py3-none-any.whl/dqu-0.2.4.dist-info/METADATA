check = DquReferentialIntegrityCheck(
    dqu_main,
    config={
        "column": "user_id",
        "reference_df": dqu_ref.df,
        "reference_column": "id"
    },
    run_id="c8395aa5-ecf0-40c0-9cc9-0a99e22f73a9"
)
