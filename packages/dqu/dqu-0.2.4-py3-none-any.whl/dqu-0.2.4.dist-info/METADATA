    StructField("id", IntegerType(), True),
    StructField("age", IntegerType(), True),
    StructField("country", StringType(), True)
])

data = [
    (1, 25, "US"),
    (2, 17, "US"),  # Fails: age < 18
    (3, 35, "CA"),  # Fails: country != US
    (4, 45, "IN")   # Fails: country != US
