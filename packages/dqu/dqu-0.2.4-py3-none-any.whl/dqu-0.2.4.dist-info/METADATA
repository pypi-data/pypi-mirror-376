env = StreamExecutionEnvironment.get_execution_environment()
env.set_parallelism(1)

# Main dataset (e.g., transactions)
main_data = [
    (101, 50.0),
    (102, 75.5),
    (103, 23.0),
    (104, 99.9),
]
main_columns = ["user_id", "amount"]
