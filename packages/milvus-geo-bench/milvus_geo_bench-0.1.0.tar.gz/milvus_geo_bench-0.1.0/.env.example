# Environment variables for Milvus Geo Benchmark Tool
# Copy this file to .env and set your actual values

# Milvus connection settings
# Replace with your actual Milvus server URI
MILVUS_URI=https://your-instance.api.gcp-us-west1.zillizcloud.com

# Replace with your actual Milvus API token
MILVUS_TOKEN=your-api-token-here

# Optional: Set log level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Optional: Default collection name
DEFAULT_COLLECTION=geo_bench

# Example for local Milvus server:
# MILVUS_URI=http://localhost:19530
# MILVUS_TOKEN=  # Leave empty for local server without authentication

# Example for Zilliz Cloud:
# MILVUS_URI=https://in03-12345abcdef.api.gcp-us-west1.zillizcloud.com
# MILVUS_TOKEN=db_12345abcdef:your_token_here