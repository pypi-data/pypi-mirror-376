<?xml version='1.0' encoding='utf-8'?>
<interface version="1.1">
  <object class="ttk.Frame" id="custom_plots_window">
    <property name="height">200</property>
    <property name="padding">10</property>
    <property name="width">200</property>
    <layout manager="grid">
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
    </layout>
    <child>
      <object class="ttk.Label" id="label1">
        <property name="font">{Segoe UI} 16 {bold}</property>
        <property name="text" translatable="yes">Plots</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="columnspan">3</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">ew</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="set_custom_plot">
        <property cbtype="simple" name="command" type="command">on_set_custom_plot</property>
        <property name="text" translatable="yes">Save</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="pady">20</property>
          <property name="propagate">True</property>
          <property name="row">23</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="cancel_custom_ep">
        <property cbtype="simple" name="command" type="command">on_cancel_custom_plot</property>
        <property name="text" translatable="yes">Cancel</property>
        <layout manager="grid">
          <property name="column">1</property>
          <property name="columnspan">2</property>
          <property name="propagate">True</property>
          <property name="row">23</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Checkbutton" id="summary_cb">
        <property name="offvalue">0</property>
        <property name="onvalue">1</property>
        <property name="state">normal</property>
        <property name="text" translatable="yes">All tags</property>
        <property name="variable">boolean:summary_cb_state</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="columnspan">3</property>
          <property name="propagate">True</property>
          <property name="row">10</property>
          <property name="sticky">ew</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Checkbutton" id="tags_detailed_cb">
        <property name="offvalue">0</property>
        <property name="onvalue">1</property>
        <property name="state">normal</property>
        <property name="text" translatable="yes">Detailed tags analysis</property>
        <property name="variable">boolean:tags_detailed_cb_state</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="columnspan">3</property>
          <property name="propagate">True</property>
          <property name="row">12</property>
          <property name="sticky">ew</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Checkbutton" id="analysis_plot_cb">
        <property name="offvalue">0</property>
        <property name="onvalue">1</property>
        <property name="state">normal</property>
        <property name="text" translatable="yes">analysis_plot</property>
        <property name="variable">boolean:analysis_plot_cb_state</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">20</property>
          <property name="propagate">True</property>
          <property name="row">13</property>
          <property name="sticky">ew</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Checkbutton" id="rx_tx_intervals_cb">
        <property name="offvalue">0</property>
        <property name="onvalue">1</property>
        <property name="text" translatable="yes">rx_tx_intervals</property>
        <property name="variable">boolean:rx_tx_intervals_cb_state</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">20</property>
          <property name="propagate">True</property>
          <property name="row">14</property>
          <property name="sticky">ew</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Checkbutton" id="wkup_metrics_cb">
        <property name="offvalue">0</property>
        <property name="onvalue">1</property>
        <property name="text" translatable="yes">wkup_metrics</property>
        <property name="variable">boolean:wkup_metrics_cb_state</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">20</property>
          <property name="propagate">True</property>
          <property name="row">15</property>
          <property name="sticky">ew</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Checkbutton" id="aux_meas_metrics_cb">
        <property name="offvalue">0</property>
        <property name="onvalue">1</property>
        <property name="text" translatable="yes">aux_meas_metrics</property>
        <property name="variable">boolean:aux_meas_metrics_cb_state</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">20</property>
          <property name="propagate">True</property>
          <property name="row">16</property>
          <property name="sticky">ew</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Checkbutton" id="lo_dco_metrics_cb">
        <property name="offvalue">0</property>
        <property name="onvalue">1</property>
        <property name="text" translatable="yes">lo_dco_metrics</property>
        <property name="variable">boolean:lo_dco_metrics_cb_state</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">20</property>
          <property name="propagate">True</property>
          <property name="row">17</property>
          <property name="sticky">ew</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Checkbutton" id="sprinkler_metrics_cb">
        <property name="offvalue">0</property>
        <property name="onvalue">1</property>
        <property name="text" translatable="yes">sprinkler_metrics</property>
        <property name="variable">boolean:sprinkler_metrics_cb_state</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">20</property>
          <property name="propagate">True</property>
          <property name="row">18</property>
          <property name="sticky">ew</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Checkbutton" id="sym_dco_metrics_cb">
        <property name="offvalue">0</property>
        <property name="onvalue">1</property>
        <property name="text" translatable="yes">sym_dco_metrics</property>
        <property name="variable">boolean:sym_dco_metrics_cb_state</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="columnspan">3</property>
          <property name="padx">20</property>
          <property name="propagate">True</property>
          <property name="row">19</property>
          <property name="sticky">ew</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Checkbutton" id="sensing_metrics_cb">
        <property name="offvalue">0</property>
        <property name="onvalue">1</property>
        <property name="text" translatable="yes">sensing_metrics</property>
        <property name="variable">boolean:sensing_metrics_cb_state</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="columnspan">3</property>
          <property name="padx">20</property>
          <property name="propagate">True</property>
          <property name="row">20</property>
          <property name="sticky">ew</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Checkbutton" id="temp_comp_and_tuning_cb">
        <property name="offvalue">0</property>
        <property name="onvalue">1</property>
        <property name="text" translatable="yes">temp_comp_and_tuning</property>
        <property name="variable">string:temp_comp_and_tuning_cb_state</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="columnspan">3</property>
          <property name="padx">20</property>
          <property name="propagate">True</property>
          <property name="row">21</property>
          <property name="sticky">ew</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Separator" id="separator1">
        <property name="orient">horizontal</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="columnspan">3</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
          <property name="sticky">ew</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Checkbutton" id="auto_open_cb">
        <property name="offvalue">0</property>
        <property name="onvalue">1</property>
        <property name="text" translatable="yes">Open plots</property>
        <property name="variable">boolean:auto_open_cb_state</property>
        <layout manager="grid">
          <property name="column">2</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">ew</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
