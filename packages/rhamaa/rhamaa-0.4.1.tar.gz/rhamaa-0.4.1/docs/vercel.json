{
  "build": {
    "env": {
      "PYTHON_VERSION": "3.9"
    }
  },
  "buildCommand": "cd docs && pip install -r requirements.txt && mkdocs build",
  "outputDirectory": "docs/site",
  "installCommand": "pip install -r docs/requirements.txt",
  "devCommand": "cd docs && mkdocs serve",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}