# fonts
font.size: 12.0
xtick.labelsize: 12.0
ytick.labelsize: 12.0
legend.fontsize: 12.0

# modified dashed_pattern
lines.dashed_pattern: 10, 5

# markers
lines.markersize: 8

# grids
grid.color: d9e1fc
