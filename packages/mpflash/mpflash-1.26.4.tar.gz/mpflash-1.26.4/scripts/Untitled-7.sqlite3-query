    SELECT  DISTINCT board_id , "exact" as match
    FROM board_downloaded
    WHERE description IN ('ESP module (1M) with ESP8266' , 'ESP module (1M)' ) 
    AND version = 'v1.21.0'

    UNION ALL

    SELECT  DISTINCT board_id, "any" as match
    FROM board_downloaded
    WHERE description IN ('ESP module (1M) with ESP8266' , 'ESP module (1M)' ) 
    --AND version = 'v1.21.0'
